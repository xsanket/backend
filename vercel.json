{
    "version": 2,
    "builds": [
      {
        "src": "./app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        {
          "src": "/api/(.*)",
          "dest": "/app.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
        },
        {
          "src": "/(.*)",
          "dest": "/app.js"
        }
      ],
    "outputDirectory": "."
  }